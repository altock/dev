    "def random_walk(n):\n",
    "  position = 0\n",
    "  walk = [position]\n",
    "  for i in range(n):\n",
    "    position += 2*random.randint(0,1) - 1\n",
    "    walk.append(position)\n",
    "  return walk"
   ]
